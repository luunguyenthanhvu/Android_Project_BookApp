<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android_BookApp"
        tools:targetApi="31">
        <activity
            android:name=".CartUser.Activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CartUser.Activity.ReviewYourOrder"
            android:exported="false"></activity>
        <activity android:name=".CartUser.Activity.MyCart"
            android:exported="false"></activity>
        <activity android:name=".CartUser.Activity.DeliveryAddress"
            android:exported="false"></activity>
        <activity android:name=".CartUser.Activity.Payment"
            android:exported="false"></activity>
        <activity android:name=".CartUser.Activity.OrderSummary"
            android:exported="false"></activity>
        <activity android:name=".CartUser.Activity.DiscountsActivity"
            android:exported="false"></activity>
    </application>

</manifest>